<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- regular -->
    <font
        android:font="@font/noto_sans_tc_thin"
        android:fontStyle="normal"
        android:fontWeight="100"

        app:font="@font/noto_sans_tc_thin"
        app:fontStyle="normal"
        app:fontWeight="100" />

    <font
            android:font="@font/noto_sans_tc_light"
            android:fontStyle="normal"
            android:fontWeight="300"

            app:font="@font/noto_sans_tc_light"
            app:fontStyle="normal"
            app:fontWeight="300" />

    <font
            android:font="@font/noto_sans_tc_regular"
            android:fontStyle="normal"
            android:fontWeight="400"

            app:font="@font/noto_sans_tc_regular"
            app:fontStyle="normal"
            app:fontWeight="400" />

    <font
            android:font="@font/noto_sans_tc_medium"
            android:fontStyle="normal"
            android:fontWeight="500"

            app:font="@font/noto_sans_tc_medium"
            app:fontStyle="normal"
            app:fontWeight="500" />

    <font
            android:font="@font/noto_sans_tc_bold"
            android:fontStyle="normal"
            android:fontWeight="700"

            app:font="@font/noto_sans_tc_bold"
            app:fontStyle="normal"
            app:fontWeight="700" />

    <font
            android:font="@font/noto_sans_tc_black"
            android:fontStyle="normal"
            android:fontWeight="900"

            app:font="@font/noto_sans_tc_black"
            app:fontStyle="normal"
            app:fontWeight="900" />

</font-family>